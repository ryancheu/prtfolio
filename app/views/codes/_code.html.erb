<!-- Using code formatting from https://github.com/blairvanderhoof/gist-embed -->
<% unless code.gist_id %>
<pre class="brush: js"> 
    <%= code.content %>
</pre>
<% else %>
<% puts "Rendering code with gist id #{code.gist_id}" %>
<code data-gist-id=<%= code.gist_id %> >
</code>
<% end %>
