  <div class="jumbotron" style="background-color:#FAD69B">
    <div class = "row">

    <div class="col-md-5" >
      <% unless project.image.nil? %>
        <%= image_tag(project.image.content.url alt="Sample Image", :class => "portfolio_image") %>
      <% else  %>
        <img src= <%= "/test.jpg" %> alt="Sample Image" class="portfolio_image">
      <% end  %>
    </div>
    <div class = "col-md-5 col-md-offset-2">
        <h2> <%= project.title %> </h1>
        <h5> <%= project.description %> </h5>
    </div>
  </div></div>
