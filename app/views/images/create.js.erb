console.log("Gist Created");
<% if @image.errors.any? %>
    console.log('Error');
    $("#resource-select-modal-body").html('<%= j render partial: "form", locals: {gist_ids: @gist_ids} %>');

<% else %>
    console.log('Image Linked');

    $('#resource-select-modal').modal('hide');
    var image = "<%= escape_javascript(render(partial: @image)) %>";
    update_block_with_resource("<%= @block.id %>", image, <%= @res_pos %>, "Image", "<%= @image.id %>");

<% end %>